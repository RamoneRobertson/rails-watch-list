<div class="bookmark-form-container">
  <div class="col-6">
    <h1>Add new bookmark for <%= @list.name %></h1>
    <%= simple_form_for([@list, @bookmark]) do |f| %>
      <%= f.input :comment %>
      <%= f.association :movie %>
      <%= f.submit class: "btn btn-dark" %>
    <% end %>
  </div>
</div>
